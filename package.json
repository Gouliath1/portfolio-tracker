{
  "name": "portfolio-tracker",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.6.1",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@portfolio/web",
    "dev:mobile": "turbo run dev --filter=@portfolio/mobile",
    "build": "turbo run build",
    "start": "turbo run start --filter=@portfolio/web",
    "lint": "turbo run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "push": "./scripts/gitPush.sh",
    "setup": "node scripts/setup.js",
    "setup:unix": "./scripts/setup.sh",
    "setup:windows": "scripts/setup.bat"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "turbo": "^2.1.3",
    "typescript": "^5"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
